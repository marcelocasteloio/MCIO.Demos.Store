{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HealthCheck": {
    "StartupPath": "/health/startup",
    "ReadinessPath": "/health/readiness",
    "LivenessPath": "/health/liveness"
  },
  "OpenTelemetry": {
    "GrpcCollectorReceiverUrl": "http://mcio-demos-store-otel-collector:4317",
    "MaxQueueSize": 2048,
    "ExporterTimeoutMilliseconds": 5000,
    "MaxExportBatchSize": 512,
    "ScheduledDelayMilliseconds": 5000
  },
  "Services": {
    "HttpServiceCollection": {
      "AnalyticsContext": {
        "BaseUrl": "http://contexts-analytics.mcio-demos-store-nginx"
      },
      "BasketContext": {
        "BaseUrl": "http://contexts-basket.mcio-demos-store-nginx"
      },
      "CalendarContext": {
        "BaseUrl": "http://contexts-calendar.mcio-demos-store-nginx"
      },
      "CatalogContext": {
        "BaseUrl": "http://contexts-catalog.mcio-demos-store-nginx"
      },
      "CustomerContext": {
        "BaseUrl": "http://contexts-customer.mcio-demos-store-nginx"
      },
      "DeliveryContext": {
        "BaseUrl": "http://contexts-delivery.mcio-demos-store-nginx"
      },
      "IdentityContext": {
        "BaseUrl": "http://contexts-identity.mcio-demos-store-nginx"
      },
      "NotificationContext": {
        "BaseUrl": "http://contexts-notification.mcio-demos-store-nginx"
      },
      "OrderContext": {
        "BaseUrl": "http://contexts-order.mcio-demos-store-nginx"
      },
      "PaymentContext": {
        "BaseUrl": "http://contexts-payment.mcio-demos-store-nginx"
      },
      "PricingContext": {
        "BaseUrl": "http://contexts-pricing.mcio-demos-store-nginx"
      },
      "ProductContext": {
        "BaseUrl": "http://contexts-product.mcio-demos-store-nginx"
      }
    }
  },
  "Token": {
    "PrivateKey": "MarceloCastelo.IO_MarceloCastelo.IO", // 8 bits per characters. We need up to 256 bits (up to 32 characters)
    "ExpiresInSeconds": 30, // 8 hours
    "Issuer": "MCIO.Demos.Store.Identity.WebApi",
    "AudienceCollection": [
      "MCIO.Demos.Store.Gateways.General"
    ]
  }
}
